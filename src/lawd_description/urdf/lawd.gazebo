    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
  <joint name="first_frontleftsec">
    <command_interface name="position">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
  <joint name="first_backleftsec">
    <command_interface name="position">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
  <joint name="first_frontrightsec">
    <command_interface name="position">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
  <joint name="first_backrightsec">
    <command_interface name="position">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
  <joint name="base_frontleftwing">
    <command_interface name="position">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
  <joint name="base_backleftwing">
    <command_interface name="position">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
  <joint name="base_frontrightwing">
    <command_interface name="position">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
  <joint name="base_backrightwing">
    <command_interface name="position">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>

  <joint name="wing_frontleftprop">
    <command_interface name="velocity">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
  <joint name="wing_backleftprop">
    <command_interface name="velocity">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
  <joint name="wing_frontrightprop">
    <command_interface name="velocity">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
  <joint name="wing_backrightprop">
    <command_interface name="velocity">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
</ros2_control>


<gazebo reference="base_link">
  <material>Gazebo/White</material>
  <mu1>0.2</mu1>
  <mu2>0.2</mu2>
  <self_collide>true</self_collide>
  <gravity>true</gravity>
</gazebo>

