<?xml version="1.0" ?>
<robot name="lawd" xmlns:xacro="http://www.ros.org/wiki/xacro" >

<ros2_control name="GazeboSystem" type="system">
  <hardware>
    <plugin>gazebo_ros2_control/GazeboSystem</plugin>
  </hardware>
  <joint name="ext_frontleftfirst">
    <command_interface name="position">
    </command_interface>
    <state_interface name="velocity"/>
    <state_interface name="position"/>
  </joint>
  <joint name="ext_backleftfirst">
    <command_interface name="position">
    </command_interface>
    <state_interface name="velocity"/>
